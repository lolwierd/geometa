{
  "name": "geometa-gallery",
  "version": "2.0.0",
  "description": "Personal GeoGuessr meta collection and study tool - simplified and streamlined",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:static": "NEXT_CONFIG_FILE=next.config.static.mjs npm run build:static:clean && mv src/app/api temp.api && next build && mv temp.api src/app/api && npm run build:static:config",
    "build:static:clean": "rm -rf .next out 2>/dev/null || true",
    "build:static:config": "cp static-config/_redirects static-config/_headers out/ 2>/dev/null || true",
    "start": "next start",
    "lint": "next lint && npm run type-check",
    "type-check": "tsc --noEmit",
    "db:init": "node db/schema-new.mjs",
    "db:migrate": "node db/schema-new.mjs",
    "db:backup": "cp db/geometa.db db/geometa-backup-$(date +%Y%m%d-%H%M%S).db",
    "test": "vitest run"
  },
  "keywords": [
    "geoguessr",
    "meta",
    "geography",
    "learning",
    "collection"
  ],
  "author": "GeoMeta Gallery",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "better-sqlite3": "^12.2.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "countries-list": "^3.1.1",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.535.0",
    "next": "^15.4.7",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-window": "^1.8.11",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "jsdom": "^24.0.0",
    "postcss": "^8.5.6",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "local"
  }
}
